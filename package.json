{
  "name": "supabase-management-api",
  "version": "1.0.0",
  "description": "Supabase Management API with Express.js",
  "main": "src/server.js",
  "type": "module",
  "scripts": {
    "dev": "nodemon src/server.js",
    "dev:memory": "node --expose-gc src/server.js",
    "start": "node src/server.js",
    "supabase:start": "npx supabase start",
    "supabase:stop": "npx supabase stop",
    "supabase:status": "npx supabase status",
    "supabase:reset": "npx supabase db reset",
    "supabase:studio": "npx supabase studio",
    "migration:new": "npx supabase migration new",
    "migration:up": "npx supabase db push",
    "migration:list": "npx supabase migration list",
    "seed": "node src/database/seed.js",
    "logs:clean": "node src/scripts/cleanLogs.js",
    "audit:clean": "node src/scripts/cleanAuditLogs.js",
    "keys:generate": "node src/scripts/generateKeys.js",
    "keys:append": "node src/scripts/appendKeys.js",
    "generate:controller": "node src/generators/controllerGenerator.js",
    "generate:route": "node src/generators/routeGenerator.js",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint src/**/*.js --fix",
    "format": "prettier --write \"src/**/*.{js,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,json,md}\"",
    "prepare": "husky install",
    "test": "NODE_ENV=test jest --runInBand",
    "test:watch": "NODE_ENV=test jest --watch",
    "test:coverage": "NODE_ENV=test jest --coverage",
    "test:unit": "NODE_ENV=test jest --testPathPattern=tests/unit",
    "test:integration": "NODE_ENV=test jest --testPathPattern=tests/integration",
    "setup": "node setup.js",
    "create": "node src/cli/create.js",
    "g": "node src/cli/create.js",
    "generate": "node src/cli/create.js",
    "install:integration": "node src/cli/install-integration.js",
    "add": "node src/cli/install-integration.js",
    "install:config": "node src/cli/install-config.js",
    "install:observability": "node src/cli/install-observability.js",
    "install:security": "node src/cli/install-security.js",
    "install:database": "node src/cli/install-database.js",
    "install:testing": "node src/cli/install-testing.js",
    "install:performance": "node src/cli/install-performance.js",
    "install:compliance": "node src/cli/install-compliance.js",
    "add:enterprise": "node src/cli/add-enterprise-feature.js",
    "enterprise": "node src/cli/add-enterprise-feature.js"
  },
  "keywords": [
    "supabase",
    "express",
    "api",
    "authentication"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@supabase/supabase-js": "^2.39.0",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "inquirer": "^9.3.8",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.3"
  }
}
